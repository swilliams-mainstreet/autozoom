{
  "manifest_version": 3,
  "name": "Zoom 200%",
  "description": "This extension automatically zooms in to 200% for Chrome users.",
  "version": "1.0.0",
  "icons": {
    "128": "resources/icon-128.png"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "38": "resources/icon-active-38.png"
    }
  }
}
